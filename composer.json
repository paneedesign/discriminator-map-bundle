{
    "name": "paneedesign/discriminator-map-bundle",
    "type": "symfony-bundle",
    "description": "Dynamic DiscriminatorMap extender for Symfony with Doctrine ORM",
    "keywords": ["symfony", "doctrine", "orm", "discriminator", "bundle", "php"],
    "homepage": "http://www.paneedesign.com/",
    "license": "MIT",
    "authors": [{
        "name": "Fabiano Roberto",
        "email": "fabiano@paneedesign.com"
    }],
    "require": {
        "php": "^5.6|^7.0",
        "symfony/framework-bundle": "^3.0|^4.0"
    },
    "suggest":     {
        "doctrine/doctrine-bundle": "^1.6"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "PaneeDesign\\DiscriminatorMapBundle\\": "" },
        "exclude-from-classmap": ["/Tests/"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
